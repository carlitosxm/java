		auto1.precio=1400.00;
		
		auto2.marca="BMW";
		auto2.anio=2020;
		auto2.precio=35000.00;
		
		System.out.println("AUTO 1");
		System.out.println("Marca: "+auto1.marca);
		System.out.println("AÃ±o: "+auto2.anio);
		System.out.println("Precio: "+auto2.precio);
		

	}

}
